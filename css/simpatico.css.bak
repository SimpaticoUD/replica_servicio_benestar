    /************************* SIMPATICO BAR**********************************/

    #simp-bar {
        display: inline-table;
        z-index: 999;
        position: fixed;
        top:0;
        width: 100%;
        height: 52px;
        background-color: #009ee0 !important;
        box-shadow: 0px 5px 5px rgba(0, 0, 0, 0.5);
      }
  
      #simp-bar img {
          vertical-align: middle;
      }
  
      /* Private buttons container */
      #simp-bar-container-left {
        display: inline-block;
        margin-top: 0;
        margin-bottom: 0;
      }
  
      /* Login button container */
      #simp-bar-container-right {
        display: inline-block;
        margin-top: 0;
        margin-bottom: 0;
        float: right;
      }
      /* Toolbar divs and containers */
      #simp-bar div, #simp-bar ul {
        display: inline-block;
        margin-top: 0;
        margin-bottom: 0;
      }
  
      /* Generic style for all the buttons */
      #simp-bar li {
          border: 1px solid transparent;
          display: inline-block;
          margin-right: 15px;
      }
  
      /* Personalised style for buttons */
  
      /* >>> QAE Button */
      .simp-bar-btn-inactive {
        background-color: transparent;
        border: 0px solid #fff;
      }
      .simp-bar-btn-inactive:hover {
          background-color: #24BCDA;
          border: 1px solid #fff;
          animation-name: anim-inactive-hover;
          animation-duration: 1s;
      }
      @keyframes anim-inactive-hover {
        from {background-color: transparent;}
        to {background-color: #24BCDA;}
      }
      .simp-bar-btn-active {
        background-color: #24BCDA;
        border: 1px solid #fff;
      }
      /* <<< QAE Button */
  
      /* >>> TAE Button */
      .simp-bar-btn-inactive-tae {
        background-color: transparent;
        border: 0px solid #fff;
      }
      .simp-bar-btn-inactive-tae:hover {
          background-color: #DE453E;
          border: 1px solid #fff;
          animation-name: anim-inactive-hover-tae;
          animation-duration: 1s;
      }
      @keyframes anim-inactive-hover-tae {
        from {background-color: transparent;}
        to {background-color: #DE453E;}
      }
      .simp-bar-btn-active-tae {
        background-color: #DE453E;
        border: 1px solid #fff;
      }
      /* <<< TAE Button */
  
